{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ignorePatterns": ["!.*", "dist", "node_modules"],
	"plugins": ["react"],
	"rules": { "import/prefer-default-export": "off" }
}
